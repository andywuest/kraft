<div id="inhalt">
<h2>{{HEADLINE}}</h2>
<p>{{DATE_LABEL}}: {{DATE}}</p>
{{#PROJECT_INFO}}
<p>{{PROJECT_LABEL}}: {{PROJECT}}</p>
{{/PROJECT_INFO}}
  {{#PRINTED}}
<p>Last printed: {{LAST_PRINT_DATE}}&nbsp;<a href="http://localhost/show_last_print?id={{LAST_PRINTED_ID}}" title="opens last creatd PDF document">[show]</a></p>
<p>{{ARCHIVED_COUNT}} older prints</p>
   {{/PRINTED}}
{{#NEVER_PRINTED}}
<p>This document was never printed</p>
{{/NEVER_PRINTED}}
</div>
